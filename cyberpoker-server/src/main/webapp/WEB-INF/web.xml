<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Austin Code Dojo CyberPoker Server</display-name>
  <servlet>
  	<servlet-name>Cyberpoker Server</servlet-name>
  	<servlet-class>com.sun.jersey.spi.container.servlet.ServletContainer</servlet-class>
  	<init-param>
  		<param-name>com.sun.jersey.config.property.packages</param-name>
  		<param-value>austincodedojo.cyberpoker.server</param-value>
  	</init-param>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Cyberpoker Server</servlet-name>
  	<url-pattern>/cyberpoker-casino/*</url-pattern>
  </servlet-mapping>
</web-app>
